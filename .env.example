
# SECRET_KEY: Django secret key (string)
SECRET_KEY=
# DEBUG: Enable debug mode (True/False)
DEBUG=
# ALLOWED_HOSTS: Comma-separated list of allowed hosts (e.g. localhost,127.0.0.1)
ALLOWED_HOSTS=

# Neo4j Password
NEO4J_PASSWORD=
# NEOMODEL_NEO4J_BOLT_URL: Neo4j connection string (e.g. bolt://neo4j:password@host:7687)
NEOMODEL_NEO4J_BOLT_URL=
# NEOMODEL_DATABASE: Neo4j database name (default: neo4j)
NEOMODEL_DATABASE=
# NEOMODEL_SIGNALS: Enable neomodel signals (True/False)
NEOMODEL_SIGNALS=
# NEOMODEL_FORCE_TIMEZONE: Force timezone handling (True/False)
NEOMODEL_FORCE_TIMEZONE=
# NEOMODEL_MAX_CONNECTION_POOL_SIZE: Max Neo4j connection pool size (integer)
NEOMODEL_MAX_CONNECTION_POOL_SIZE=



# JWT_ACCESS_MIN: JWT access token expiry in minutes (integer)
JWT_ACCESS_MIN=
# JWT_REFRESH_DAYS: JWT refresh token expiry in days (integer)
JWT_REFRESH_DAYS=

# Log File  0-> console, 1->File
LOG_TO_FILE= 


# CORS_ALLOW_ALL_ORIGINS: Allow all origins for CORS (True/False)
CORS_ALLOW_ALL_ORIGINS=
# CSRF_TRUSTED_ORIGINS: Trusted origins for CSRF protection (comma-separated URLs)
CSRF_TRUSTED_ORIGINS=
# CORS_ALLOWED_ORIGINS: Allowed origins for CORS (comma-separated URLs)
CORS_ALLOWED_ORIGINS=



# HTTP_X_FORWARDED_PROTO: Protocol for proxy headers (e.g. https)
HTTP_X_FORWARDED_PROTO=
# SESSION_COOKIE_SECURE: Secure session cookies (True/False)
SESSION_COOKIE_SECURE=
# SECURE_SSL_REDIRECT: Redirect all requests to HTTPS (True/False)
SECURE_SSL_REDIRECT=
# CSRF_COOKIE_SECURE: Secure CSRF cookies (True/False)
CSRF_COOKIE_SECURE=